  "scripts": {
    "start:app": "webpack --env app && tsc --project app && yarn build:dev",
    "build:dev": "cross-env ENVIRONMENT=development electron build/main.js",
    "pack:app": "electron-builder --dir --config build/builder.yml",
    "dist:app": "electron-builder --config build/builder.yml",
    "test:watch": "./node_modules/.bin/jest --watch --silent",
    "test:run": "./node_modules/.bin/jest --notify --silent",
    "start:web": "webpack-dev-server --inline --progress",
    "test:clear": "./node_modules/.bin/jest --clearCache",
    "postinstall": "electron-builder install-app-deps",
    "build:app": "yarn build:web && tsc --project app",
    "lint:css": "stylelint **/*.{svelte,scss,css}",
    "lint:js": "eslint src/**/*.{ts,tsx,js,jsx}",
    "test:cover": "yarn test:jest --coverage",
    "build:web": "webpack --env build",
    "serve": "npx http-server"
  },
