{
  "name": "YetAnotherZombieHorror",
  "description": "Yet Another Zombie Horror",
  "homepage": "https://github.com/UstymUkhman/YetAnotherZombieHorror#readme",
  "author": "Ustym Ukhman <ustym.ukhman@gmail.com> (https://github.com/UstymUkhman/)",
  "packageManager": "^pnpm@7.13.4",
  "main": "src/main.ts",
  "version": "0.7.0",
  "license": "MIT",
  "private": false,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/UstymUkhman/YetAnotherZombieHorror.git"
  },
  "bugs": {
    "url": "https://github.com/UstymUkhman/YetAnotherZombieHorror/issues",
    "email": "ustym.ukhman@gmail.com"
  },
  "keywords": [
    "fps",
    "tps",
    "game",
    "horror",
    "zombies",
    "shooter",
    "survival",
    "survival-horror",
    "survival-shooter",
    "first-person-shooter",
    "third-person-shooter",
    "zombie-survival-shooter"
  ],
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "setup": "node ./build/index.mjs",
    "start:web": "vite",
    "build:web": "vite build",
    "serve:web": "vite preview",
    "build:prod": "set prod=true&& vite build",
    "start:app": "pnpm build:app && pnpm build:dev",
    "build:app": "pnpm build:web && tsc --project app",
    "build:dev": "set ENVIRONMENT=development&& electron build/app/main.js",
    "pack:app": "electron-builder --dir --config app/builder.yml",
    "dist:app": "electron-builder --config app/builder.yml",
    "lint:js": "eslint src/**/*.{svelte,ts,js}",
    "lint:css": "stylelint **/*.{svelte,scss,css}",
    "test:cover": "vitest run --coverage",
    "test:run": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "ammo.js": "^0.0.10",
    "animejs": "^3.2.1",
    "raindrop-fx": "^1.0.8",
    "three": "^0.145.0",
    "three-mesh-bvh": "^0.5.16"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "1.0.9",
    "@types/animejs": "^3.1.5",
    "@types/node": "^18.8.5",
    "@types/three": "^0.144.0",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "electron": "^21.1.1",
    "electron-builder": "^23.6.0",
    "eslint": "^8.25.0",
    "fake-indexeddb": "^4.0.0",
    "jest-canvas-mock": "^2.4.0",
    "jsdom": "^20.0.1",
    "postcss": "^8.4.18",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.5",
    "sass": "^1.55.0",
    "stylelint": "^14.13.0",
    "stylelint-config-standard": "^28.0.0",
    "svelte": "^3.51.0",
    "svelte-preprocess": "^4.10.7",
    "typescript": "^4.8.4",
    "vite": "^3.1.8",
    "vite-plugin-glsl": "0.5.1",
    "vitest": "^0.24.3"
  },
  "engines": {
    "node": ">= 16.15.1",
    "npm": ">= 8.11.0"
  }
}
