{
  "name": "another-dumb-zombie-game",
  "description": "Another Dumb Zombie Game",
  "author": "Ustym Ukhman <ustym.ukhman@gmail.com>",
  "homepage": "https://github.com/UstymUkhman/another-dumb-zombie-game#readme",
  "keywords": [],
  "main": "build/main.js",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "start:app": "yarn build:app && electron build/main.js",
    "start:web": "webpack-dev-server --inline --progress",
    "postinstall": "electron-builder install-app-deps",
    "test:jest": "./node_modules/.bin/jest --notify",
    "lint:js": "eslint src/**/*{.ts,.tsx,.js,.jsx}",
    "lint:css": "stylelint **/*.{svelte,html,css}",
    "test:cover": "yarn test:jest --coverage",
    "pack:app": "electron-builder --dir",
    "build:web": "webpack --env build",
    "build:app": "tsc --project app",
    "dist:app": "electron-builder",
    "serve": "npx http-server"
  },
  "build": {
    "appId": "another.dumb.zombie.game",
    "publish": null,
    "files": [
      "build/**/*",
      "public/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "output": "build",
      "buildResources": "favicon"
    },
    "win": {
      "legalTrademarks": "Ustym Ukhman Â© 2020",
      "icon": "favicon/icon-512x512.png",
      "publisherName": "Ustym Ukhman",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/UstymUkhman/another-dumb-zombie-game.git"
  },
  "bugs": {
    "url": "https://github.com/UstymUkhman/another-dumb-zombie-game/issues"
  },
  "dependencies": {
    "@types/animejs": "^3.1.1",
    "@types/lodash": "^4.14.153",
    "animejs": "^3.2.0",
    "three": "^0.117.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.10.1",
    "@types/electron": "^1.6.10",
    "@types/jest": "^25.2.3",
    "@types/node": "12.12.21",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "babel-jest": "^26.0.1",
    "css-loader": "^3.5.3",
    "electron-builder": "^22.7.0",
    "eslint": "^7.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^4.0.2",
    "file-loader": "^6.0.0",
    "gltf-loader-2": "^0.0.3",
    "jest": "^26.0.1",
    "mini-css-extract-plugin": "^0.9.0",
    "minimist": "^1.2.5",
    "stylelint": "^13.5.0",
    "stylelint-config-standard": "^20.0.0",
    "svelte": "^3.23.0",
    "svelte-loader": "^2.13.6",
    "terser-webpack-plugin": "^3.0.2",
    "ts-jest": "^26.0.0",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.3",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "worker-loader": "^2.0.0"
  },
  "resolutions": {
    "minimist": "^1.2.5"
  },
  "engines": {
    "node": ">= 9.10.0",
    "npm": ">= 5.5.0"
  }
}
