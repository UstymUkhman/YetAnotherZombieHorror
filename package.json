    "@types/animejs": "^3.1.4",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.7",
    "@types/three": "^0.134.0",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "babel-jest": "^27.3.1",
    "cross-env": "^7.0.3",
    "electron": "^16.0.0",
    "electron-builder": "^22.13.1",
    "eslint": "^8.2.0",
    "jest": "^27.3.1",
    "jest-canvas-mock": "^2.3.1",
    "node-notifier": "^10.0.0",
    "postcss": "^8.3.11",
    "postcss-html": "^1.2.0",
    "postcss-scss": "^4.0.2",
    "sass": "^1.43.4",
    "stylelint": "^14.0.1",
    "stylelint-config-standard": "^24.0.0",
    "svelte": "^3.44.1",
    "svelte-preprocess": "^4.9.8",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4",
    "vite": "^2.6.14",
    "vite-plugin-glsl": "^0.0.5"
  },
  "engines": {
    "node": ">= 14.17.0",
    "npm": ">= 6.14.13"
  }
