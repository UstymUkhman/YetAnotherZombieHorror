    "@types/animejs": "^3.1.4",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.38",
    "@types/three": "^0.141.0",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "babel-jest": "^27.5.1",
    "electron": "^19.0.1",
    "electron-builder": "^23.0.3",
    "eslint": "^8.16.0",
    "fake-indexeddb": "^3.1.7",
    "jest": "^27.5.1",
    "jest-canvas-mock": "^2.4.0",
    "jest-environment-jsdom": "^27.5.1",
    "node-notifier": "^10.0.1",
    "postcss": "^8.4.14",
    "postcss-html": "^1.4.1",
    "postcss-scss": "^4.0.4",
    "sass": "^1.52.1",
    "stylelint": "^14.8.5",
    "stylelint-config-standard": "^25.0.0",
    "svelte": "^3.48.0",
    "svelte-preprocess": "^4.10.6",
    "ts-jest": "^27.1.4",
    "typescript": "^4.7.2",
    "vite": "^2.9.9",
    "vite-plugin-glsl": "0.1.1"
  },
  "engines": {
    "node": ">= 14.17.0",
    "npm": ">= 6.14.13"
  }
}
